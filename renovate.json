{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "matchManagers": ["github-actions"],
      "matchUpdateTypes": ["digest", "pin", "minor", "patch"],
      "automerge": true,
      "pinDigests": true
    }
  ]
}
